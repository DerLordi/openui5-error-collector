{
  "compilerOptions": {
    "module": "none",
    "target": "es2018",
    "noEmit": true,
    "checkJs": true,
    "strict": true,
    "allowJs": true,
    "declaration": true,
    "emitDeclarationOnly": false,
    "skipLibCheck": true,
    "outDir": "types",
    "types": [
      "openui5",
      "node"
    ]
  },
  "include": ["src/**/*"],
  "exclude": ["test/**/*", "node_modules"]
}
