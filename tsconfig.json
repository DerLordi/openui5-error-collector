{
  "compilerOptions": {
    "module": "none",
    "target": "es2018",
    "noEmit": true,
    "checkJs": true,
    "strict": true,
    "allowJs": true,
    "declaration": true,
    "emitDeclarationOnly": false,
    "skipLibCheck": true,
    "outDir": "types",
    "types": [
      "@openui5/ts-types",
      "jasmine",
      "node",
      "qunit",
      "sinon"
    ]
  },
  "include": ["webapp/**/*"],
  "exclude": ["webapp/test/**/*", "node_modules"]
}
