{
	"_version": "1.36.0",
  "start_url": "index.html",
	"sap.app": {
		"id": "ui5.template.boilerplate",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "${version}"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"dataSources": {
			"mainService": {
				"uri": "https://services.odata.org/V2/Northwind/Northwind.svc",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/UTILITIES_SRV/metadata.xml"
				}
			}
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"fullWidth": true
	},
	"sap.ui5": {
		"handleValidation": true,
		"flexEnabled": false,
		"rootView": {
			"viewName": "ui5.template.boilerplate.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.96.0",
			"libs": {
				"sap.m": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ui5.template.boilerplate.i18n.i18n"
				}
			},
			"app": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"device": {
				"type": "sap.ui.model.json.JSONModel"
			},
			"": {
				"type": "sap.ui.model.odata.v2.ODataModel",
				"dataSource": "mainService",
				"preload": true,
        "settings": {
          "defaultCountMode": "InlineRepeat",
					"disableHeadRequestForToken": true
        }
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"async": true,
				"viewPath": "ui5.template.boilerplate.view",
				"controlId": "app",
				"controlAggregation": "Pages",
				"clearControlAggregation": true
			},
			"routes": [],
			"targets": {}
		}
	}
}
