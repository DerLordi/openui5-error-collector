    "lint:fix": "eslint webapp --fix"
  },
  "ui5": {
    "dependencies": [
      "ui5-task-minifier",
      "openui5-smart-mockserver"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mauriciolauffer/ui5-template-boilerplate.git"
  },
  "bugs": {
    "url": "https://github.com/mauriciolauffer/ui5-template-boilerplate/issues"
  }
