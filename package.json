{
  "name": "ui5-template-boilerplate",
  "version": "1.0.0",
  "description": "UI5 template boilerplate",
  "private": true,
  "author": "Mauricio Lauffer",
  "devDependencies": {
    "@openui5/ts-types": "^1.90.7",
    "@types/jasmine": "^3.10.0",
    "@types/node": "^16.11.1",
    "@types/qunit": "^2.11.2",
    "@types/sinon": "^10.0.4",
    "eslint": "^7.32.0",
    "eslint-config-mlauffer-ui5": "^0.3.0",
    "karma": "^6.3.4",
    "karma-ui5": "^2.3.4",
    "openui5-smart-mockserver": "^0.3.1",
    "ui5-task-minifier": "^1.0.2"
  },
  "scripts": {
    "start": "ui5 serve",
    "build": "ui5 build --clean-dest --include-task=generateCachebusterInfo",
